@model VistaEmpleado

<h1>Vista Empleado</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID EMPLEADO</th>
            <th>APELLIDO</th>
            <th>OFICIO</th>
            <th>SALARIO</th>
            <th>DEPARTAMENTO</th>
            <th>NOMBRE</th>
            <th>LOCALIDAD</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Id</td>
            <td>@Model.Apellido</td>
            <td>@Model.Oficio</td>
            <td>@Model.Salario</td>
            <td>@Model.Departamento</td>
            <td>@Model.NombreDept</td>
            <td>@Model.Localidad</td>
        </tr>
    </tbody>
</table>
